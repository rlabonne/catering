<h1>Contacts</h1>
<p> </p>
<div class="row">
  <div class="col-md-10">
    <% @contacts.each do |contact| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
              <%= link_to contact.lastName, contact %>, <%= link_to contact.firstName, contact %> 
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
